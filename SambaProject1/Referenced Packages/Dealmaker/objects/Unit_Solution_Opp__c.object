        <formula>IF( Opportunity_Type__c = &apos;Current&apos;,  
   IF( AND( Opportunity__r.CloseDate &gt;=  Plan_Unit__r.Account_Plan__r.From_Date__c ,  Opportunity__r.CloseDate &lt;=  Plan_Unit__r.Account_Plan__r.To_Date__c ) , 
