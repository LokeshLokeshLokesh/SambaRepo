    <validationRules>
        <fullName>Closure_Duration_Check</fullName>
        <active>true</active>
        <errorConditionFormula>Closure_Duration__c &lt; 0</errorConditionFormula>
        <errorDisplayField>Closure_Duration__c</errorDisplayField>
        <errorMessage>Closure duration must be positive</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Closure_duration_length</fullName>
        <active>true</active>
        <errorConditionFormula>Closure_Duration__c  &gt; 1000</errorConditionFormula>
        <errorDisplayField>Closure_Duration__c</errorDisplayField>
        <errorMessage>Closure Duration cannot be greater than 1000 days</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Percentage_check_FSP</fullName>
        <active>true</active>
        <errorConditionFormula>Final_Stage_Probability__c &gt; 1 ||  Final_Stage_Probability__c &lt; 0</errorConditionFormula>
        <errorDisplayField>Final_Stage_Probability__c</errorDisplayField>
        <errorMessage>Only 0-100% is valid</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Percentage_check_PP</fullName>
        <active>true</active>
        <errorConditionFormula>Pebble_Percentage__c  &gt; 1 || Pebble_Percentage__c  &lt; 0</errorConditionFormula>
        <errorDisplayField>Pebble_Percentage__c</errorDisplayField>
        <errorMessage>Only 0-100% is valid</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Percentage_check_PT</fullName>
        <active>true</active>
        <errorConditionFormula>Threshold__c  &gt; 1 || Threshold__c &lt; 0</errorConditionFormula>
        <errorDisplayField>Threshold__c</errorDisplayField>
        <errorMessage>Only 0-100% is valid</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Percentage_check_RP</fullName>
        <active>true</active>
        <errorConditionFormula>Rock_Percentage__c &gt; 1 || Rock_Percentage__c &lt; 0</errorConditionFormula>
        <errorDisplayField>Rock_Percentage__c</errorDisplayField>
        <errorMessage>Only 0-100% is valid</errorMessage>
    </validationRules>
