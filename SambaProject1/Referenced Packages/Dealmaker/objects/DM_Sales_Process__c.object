    <fields>
        <fullName>Total_Sales_Cycle_Duration__c</fullName>
        <deprecated>false</deprecated>
        <description>Sum of all stage duration plus sales process closure duration</description>
        <externalId>false</externalId>
        <formula>Sum_Of_Stages_Duration__c  +  Closure_Duration__c</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>Total Sales Cycle Duration</label>
        <precision>18</precision>
        <required>false</required>
