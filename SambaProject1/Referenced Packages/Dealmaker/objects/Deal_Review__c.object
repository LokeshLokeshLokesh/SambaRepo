    </fields>
    <fields>
        <fullName>Status__c</fullName>
        <deprecated>false</deprecated>
        <encrypted>false</encrypted>
        <externalId>false</externalId>
        <formula>IF( 
	Is_Complete__c , 
	&apos;Completed&apos;, 
	IF( !ISBLANK(Start_Time__c) &amp;&amp; ISBLANK(End_Time__c) ,
		&apos;Started&apos;,
		IF( 
			Effective_Date__c &lt; BLANKVALUE($Setup.Dealmaker_Core_Settings__c.Mock_Today__c, TODAY()),
			&apos;Overdue&apos;,			
			IF( 
				ISNULL( Reschedule_Date__c )|| ISNULL(Original_Schedule_Date__c) &amp;&amp; NOT(ISNULL(Reschedule_Date__c)), 
				&apos;Scheduled&apos;,
				IF(
					Reschedule_Date__c &lt;= Original_Schedule_Date__c,
					&apos;Rescheduled&apos;,
					&apos;Deferred&apos;
				)
			)
		)
	)
)</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>Status</label>
        <required>false</required>
        <trackTrending>false</trackTrending>
        <type>Text</type>
        <unique>false</unique>
