    <fields>
        <fullName>Review_Duration__c</fullName>
        <deprecated>false</deprecated>
        <description>Set duration field based on Status and difference between start and end times. Return the total number of minutes</description>
        <externalId>false</externalId>
        <formula>IF ( Status__c == &apos;Completed&apos;, 
    IF (End_Time__c - Start_Time__c &gt; 0, 
        ROUND( (End_Time__c - Start_Time__c ) * 24 * 60, 0)
        ,0
    ), 
NULL)</formula>
