    <fields>
        <fullName>StatusSortValue__c</fullName>
        <deprecated>false</deprecated>
        <externalId>false</externalId>
        <formula>IF(Status__c == &apos;Started&apos;, 
   1, 
   IF(Status__c == &apos;Overdue&apos;, 
      2, 
      IF(Status__c == &apos;Scheduled&apos;, 
         3, 
         4
      ) 
   ) 
)</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>StatusSortValue</label>
        <precision>18</precision>
        <required>false</required>
        <scale>0</scale>
