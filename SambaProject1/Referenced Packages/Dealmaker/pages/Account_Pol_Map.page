    						ttgApi.markOrgChartAsPrimary(mapId,
    			                function() { confirmDialog.dialog('close'); },                    
    			                function() { /* error */ },                 
    			                function() { /* complete */ }               
    			            )
    					},
    					'No' : function() { 
    						confirmDialog.dialog('close'); 
